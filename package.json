{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API do curso Udemy",
  "main": "server/server.js",
  "scripts": {
    "start": "cross-env PORT=5000 NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "cross-env PORT=5000 NODE_ENV=production node ./dist/server/server.js",
    "dbconfig": "cross-env NODE_ENV=test node ./dist/server/config/dbconfig.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "unit-test": "cross-env NODE_ENV=test mocha --opts ./dist/tests/unit/config/mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts ./dist/tests/integration/config/mocha.opts ./dist/tests/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test"
  },
  "keywords": [
    "API",
    "Typescript",
    "Postgree",
    "ES6",
    "Node"
  ],
  "author": "Adroaldo",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "express": "^4.17.1",
    "http": "0.0.0",
    "http-status": "^1.3.2",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.1",
    "pg": "^7.12.1",
    "sequelize": "^5.18.4",
    "sequelize-cli": "^5.5.1",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.2",
    "@types/express": "^4.17.1",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.2",
    "@types/sequelize": "^4.28.4",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.1",
    "mocha": "^6.2.0",
    "supertest": "^4.0.2",
    "testdouble": "^3.12.4",
    "ts-node": "^8.3.0"
  }
